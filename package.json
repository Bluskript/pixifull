    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.7"
